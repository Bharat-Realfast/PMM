<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>APPLICATION_DATE_BEFORE_START_DATE</fullName>
    <active>true</active>
    <description>Ensures application date is after the start date</description>
    <errorConditionFormula>ApplicationDate__c &gt; StartDate__c</errorConditionFormula>
    <errorMessage>Start Date must be after the Application Date</errorMessage>
</ValidationRule>
